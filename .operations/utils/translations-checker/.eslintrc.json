{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
            "camelcase": "error",
            "spaced-comment": "error",
            "quotes": ["error", "single", { "avoidEscape": true }],
            "no-duplicate-imports": "error"

    }
}
